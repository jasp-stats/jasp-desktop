{
	"name": "SummaryStatsRegressionLinearBayesian",
	"autorun": true,
	"version": "1.0",
	"options": [
		{
			"format": "",
			"name": "unadjustedRSquared",
			"type": "Number",
			"min": 0.00001,
			"max": 0.99999,
			"value": 0.5
		},
		{
			"name": "sampleMode",
			"type": "List",
			"options": [ "auto", "manual" ]
		},
		{
			"name": "bayesFactorType",
			"options": [ "BF10", "BF01", "LogBF10" ],
			"default": "BF10",
			"type": "List"
		},
		{
			"default": false,
			"name": "plotBayesFactorRobustness",
			"type": "Boolean"
		},
		{
			"format": "",
			"name": "sampleSize",
			"type": "Integer",
			"min": 3
		},
		{
			"format": "",
			"name": "numberOfCovariates",
			"type": "Integer",
			"min": 1
		},
		{
			"format": "",
			"name": "priorWidth",
			"type": "Number",
			"value": 0.3536,
			"min": 0,
			"max": 2
		}
	]	
}
