{
	"name": "TTestBayesianOneSample",
	"autorun": true,
	"version": "1.00",
	"options": [
		{
			"name": "variables",
			"type": "Variables"
		},
		{
			"name": "hypothesis",
			"options": [ "notEqualToTestValue", "greaterThanTestValue", "lessThanTestValue" ],
			"default": "notEqualToTestValue",
			"type": "List"
		},
		{
			"name": "missingValues",
			"options": [ "excludeAnalysisByAnalysis", "excludeListwise" ],
			"default": "excludeAnalysisByAnalysis",
			"type": "List"
		},
		{
			"name": "standardizedEffectSize",
			"default": true,
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotPriorAndPosterior",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotPriorAndPosteriorAdditionalInfo",
			"type": "Boolean"
		},
		{
			"format": "",
			"name": "testValue",
			"type": "Number",
			"value": 0.0
		},
		{
			"name": "descriptives",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotBayesFactorRobustness",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotSequentialAnalysis",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotSequentialAnalysisRobustness",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "descriptivesPlots",
			"type": "Boolean"
		},
		{
			"format": "%",
			"max": 0.999,
			"min": 0.000,
			"name": "descriptivesPlotsCredibleInterval",
			"type": "Number",
			"value": 0.950
		},
		{
			"default": 320,
			"name": "plotWidth",
			"type": "Integer"
		},
		{
			"default": 240,
			"name": "plotHeight",
			"type": "Integer"
		},
		{
			"name": "bayesFactorType",
			"options": [ "BF10", "BF01", "LogBF10" ],
			"default": "BF10",
			"type": "List"
		},
		{
			"name": "effectSize",
			"options": ["standardized", "dienes"],
			"default": "standardized",
			"type": "List"
		},
		{
			"name": "effectSizeStandardized",
			"options": ["default", "informative"],
			"default": "default",
			"type": "List"
		},
		{
			"name": "defaultStandardizedEffectSize",
			"options": ["cauchy"],
			"default": "cauchy",
			"type": "List"
		},
		{
			"format": "",
			"name": "priorWidth",
			"type": "Number",
			"value": 0.7070,
			"min": 0,
			"max": 2
		},
		{
			"name": "informativeStandardizedEffectSize",
            "options": ["cauchy", "normal","t"],
			"default": "cauchy",
			"type": "List"
		},
		{
			"format": "",
			"name": "informativeCauchyLocation",
			"type": "Number",
			"value": 0,
			"min": -3,
			"max": 3
		},
		{
			"format": "",
			"name": "informativeCauchyScale",
			"type": "Number",
			"value": 0.7070,
			"min": 0,
			"max": 2
		},
		{
			"format": "",
			"name": "informativeTLocation",
			"type": "Number",
			"value": 0,
			"min": -3,
			"max": 3
		},
		{
			"format": "",
			"name": "informativeTScale",
			"type": "Number",
			"value": 0.7070,
			"min": 0,
			"max": 2
		},
		{
			"format": "",
			"name": "informativeTDf",
			"type": "Number",
			"value": 1,
			"min": 1,
			"max": 500
		},
		{
			"format": "",
			"name": "informativeNormalMean",
			"type": "Number",
			"value": 0,
			"min": -3,
			"max": 3
		},
		{
			"format": "",
			"name": "informativeNormalStd",
			"type": "Number",
			"value": 0.7070,
			"min": 0,
			"max": 2
		},
		{
			"name": "dienesEffectSize",
			"options": ["uniform", "normal", "half_normal"],
			"default": "uniform",
			"type": "List"
		},
		{
			"format": "",
			"name": "uniformDienesLowerBound",
			"type": "Number",
			"value": 0.7070,
			"min": 0,
			"max": 2
		},
		{
			"format": "",
			"name": "uniformDienesUpperBound",
			"type": "Number",
			"value": 0.7070,
			"min": 0,
			"max": 2
		},
		{
			"format": "",
			"name": "normalDienesMean",
			"type": "Number",
			"value": 0.7070,
			"min": 0,
			"max": 2
		},
		{
			"format": "",
			"name": "normalDienesStd",
			"type": "Number",
			"value": 0.7070,
			"min": 0,
			"max": 2
		},
		{
			"format": "",
			"name": "halfNormalDienesStd",
			"type": "Number",
			"value": 0.7070,
			"min": 0,
			"max": 2
		}
	]
}
