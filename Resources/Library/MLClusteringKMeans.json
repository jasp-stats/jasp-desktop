{
	"name": "MLClusteringKMeans",
	"autorun": true,
	"version": "1.00",
	"options": [
		{
			"name": "predictors",
			"type": "Variables"
		},
		{
			"name": "noOfClusters",
			"type": "List",
			"options": [ "auto", "manual", "optimized", "robust" ],
			"default": "auto"
		},
		{
			"name": "clusterSize",
            "type": "Integer",
            "default": 2,
            "min": 1
		},
		{
			"name": "noOfIterations",
			"type": "List",
			"options": [ "auto", "manual" ],
			"default": "auto"
		},
		{
			"name": "noOfRandomSets",
			"type": "List",
			"options": [ "auto", "manual" ],
			"default": "auto"
		},
		{
			"name": "nearestNeighboursCount",
			"type": "Integer"
		},
		{
			"name": "optimizedFrom",
            "type": "Integer",
            "default": 2,
            "min": 1
		},
		{
			"name": "optimizedTo",
            "type": "Integer",
            "default": 5,
            "min": 1
		},
		{
			"name": "robustFrom",
            "type": "Integer",
            "default": 2,
            "min": 1
		},
		{
			"name": "robustTo",
            "type": "Integer",
            "default": 3,
            "min": 1
		},
		{
			"name": "iterationsCount",
            "type": "Integer",
            "default": 25,
            "min": 1
		},
		{
			"name": "randomSetCount",
            "type": "Integer",
            "default": 15,
            "min": 1
		},
		{
			"name": "plot2dCluster",
			"type": "Boolean"
		},
		{
			"name": "plot2dClusterCentroids",
			"type": "Boolean"
		},
		{
			"name": "plotCriterionVsClusters",
			"type": "Boolean"
		},
		{
			"name": "plotPCACluster",
			"type": "Boolean"
		},
		{
			"name": "plotPCAClusterSquares",
			"type": "Boolean"
		},
		{
			"name": "tableClusterInformation",
			"type": "Boolean"
		},
		{
			"name": "tableClusterInfoSize",
            "type": "Boolean",
            "default": true
		},
		{
			"name": "tableClusterInfoCentroids",
			"type": "Boolean"
		},
		{
			"name": "tableClusterInfoSumSquares",
			"type": "Boolean"
		},
		{
			"name": "tableClusterInfoTotalSumSquares",
			"type": "Boolean"
		},
		{
			"name": "tableClusterInfoBetweenSumSquares",
			"type": "Boolean"
		},
		{
			"name": "tablePredictions",
			"type": "Boolean"
		},
		{
			"name": "naAction",
			"type": "List",
			"options": [ "deleteListwise", "predict", "roughFix" ],
			"default": "deleteListwise"
		},
		{
			"name": "algorithm",
			"type": "List",
			"options": [ "hartiganWong", "lloyd", "macQueen" ],
			"default": "hartiganWong"
		},
		{
			"name": "criterion",
            "options": [ "silhoutteLength", "Multiasw", "Calinski-Harabasz"],
            "type": "List",
            "default": "silhoutteLength"
        },
        {
            "name": "predictionsFrom",
            "type": "Integer",
            "min": 1,
            "max": 999999,
            "default": 1
        },
        {
            "name": "predictionsTo",
            "type": "Integer",
            "min": 1,
            "max": 999999,
            "default": 10
        },
        {
            "default": 640,
            "name": "plotWidth",
            "type": "Integer"
        },
        {
            "default": 480,
            "name": "plotHeight",
            "type": "Integer"
        },
	]
}
