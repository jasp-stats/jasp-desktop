{
	"name": "RegressionLogLinearBayesian",
	"autorun": true,
	"version": "1.0.0",
	"options": [
		{
			"name": "counts",
			"type": "Variable"
		},
		{
			"name": "factors",
			"type": "Variables"
		},
		{
			"name": "modelTerms",
			"template": [
				{
					"name": "components",
					"type": "Term"
				}
			],
			"type": "Table"
		},
		{
			"name": "bayesFactorType",
			"type": "List",
			"options": [ "BF10", "BF01", "LogBF10" ]
		},
		{
			"name": "priorScale",
			"type": "Number",
			"value" : 0,
			"min" : 0
		},
		{
			"name": "priorShape",
			"type": "Number",
			"value" : -1,
			"min" : -1
		},
		{
			"name": "maxModels",
			"type": "Integer",
			"default": 2,
			"min" : 2
		},
		{
			"name": "posteriorProbabilityCutOff",
			"type": "Number",
			"value": 0.1,
			"max": 0.5,
			"min": 0.0
		},
		{
			"name": "sampleMode",
			"type": "List",
			"options": [ "auto", "manual" ]
		},
		{
			"name": "fixedSamplesNumber",
			"type": "Integer",
			"default": 10000,
			"min": 0
		},
		{
			"name": "regressionCoefficientsEstimates",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "regressionCoefficientsCredibleIntervals",
			"type": "Boolean",
			"default": false
		},
		{
			"format": "%",
			"max": 0.999,
			"min": 0.5,
			"name": "regressionCoefficientsCredibleIntervalsInterval",
			"type": "Number",
			"value": 0.950
		},
		{
			"name": "regressionCoefficientsSubmodel",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "regressionCoefficientsSubmodelNo",
			"type": "Integer",
			"default": 1,
			"min" : 1
		},
		{
			"name": "regressionCoefficientsSubmodelEstimates",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "regressionCoefficientsSubmodelCredibleIntervals",
			"type": "Boolean",
			"default": false
		},
		{
			"format": "%",
			"max": 0.999,
			"min": 0.5,
			"name": "regressionCoefficientsSubmodelCredibleIntervalsInterval",
			"type": "Number",
			"value": 0.950
		}
	]
}
