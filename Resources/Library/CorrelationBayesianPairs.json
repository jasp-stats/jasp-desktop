{
	"name": "CorrelationBayesianPairs",
	"autorun": true,
	"version": "1.00",
	"options": [
		{
			"name": "pairs",
			"type": "VariablesGroups"
		},
		{
			"name": "corcoefficient",
			"options": [ "Pearson", "Kendall"],
			"default": "Pearson",
			"type": "List"
		},
		{
			"name": "hypothesis",
			"options": [ "correlated", "correlatedPositively", "correlatedNegatively" ],
			"default": "correlated",
			"type": "List"
		},
		{
			"name": "missingValues",
			"options": [ "excludeAnalysisByAnalysis", "excludeListwise" ],
			"default": "excludeAnalysisByAnalysis",
			"type": "List"
		},
		{
			"format": "",
			"name": "priorWidth",
			"type": "Number",
			"value": 1.0,
			"min" : 0,
			"max" : 2.0
		},
		{
			"default": false,
			"name": "plotScatter",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotPriorAndPosterior",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotPriorAndPosteriorAdditionalInfo",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotBayesFactorRobustness",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotSequentialAnalysis",
			"type": "Boolean"
		},
		{
			"default": false,
			"name": "plotSequentialAnalysisRobustness",
			"type": "Boolean"
		},
		{
			"default": 320,
			"name": "plotWidth",
			"type": "Integer"
		},
		{
			"default": 240,
			"name": "plotHeight",
			"type": "Integer"
		},
		{
			"name": "bayesFactorType",
			"options": [ "BF10", "BF01", "LogBF10" ],
			"default": "BF10",
			"type": "List"
		},
		{
			"name": "credibleInterval",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "ciValue",
			"type": "Number",
			"format": "%",
			"value": 0.950,
			"min": 0.001,
			"max": 0.999
		}
	]
}
