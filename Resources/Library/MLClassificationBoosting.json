{
	"name":"MLClassificationBoosting",
	"autorun":true,
	"version":"1.00",
	"options":[
		{
			"name":"predictors",
			"type":"Variables"
		},
		{
			"name":"target",
			"type":"Variable"
		},
		{
			"name":"indicator",
			"type":"Variable"
		},
		{
			"default":100,
			"min": 1,
			"name":"numberOfIterations",
			"type":"Integer"
		},
		{
			"value":0.1,
			"format": "",
			"name":"Shrinkage",
			"type":"Number",
			"min":0,
			"max":1
		},
		{
			"name": "distribution",
			"options": [ "default","bernoulli","huberized","adaboost","multinomial"],
			"default": "default",
			"type": "List"
		},
		{
			"default":false,
			"name":"tableRF",
			"type":"Boolean"
		},
		{
			"default":false,
			"name":"tableChisq",
			"type":"Boolean"
		},
		{
			"default":false,
			"name":"predictNA",
			"type":"Boolean"
		},
		{
			"default":false,
			"name":"modelPerformance",
			"type":"Boolean"
		},
		{
			"default":false,
			"name":"modelPerforamceShowError",
			"type":"Boolean"
		},
		{
			"default":false,
			"name":"modelPerforamceShowAUC",
			"type":"Boolean"
		},
		{
			"default":false,
			"name":"plotRF",
			"type":"Boolean"
		},
		{
			"default":false,
			"name":"plotMarginalPlot",
			"type":"Boolean"
		},
		{
			"default":false,
			"name":"plotMarginalPlotOneWay",
			"type":"Boolean"
		},

		{
			"default":false,
			"name":"plotMarginalPlotTwoWay",
			"type":"Boolean"
		},

		
		{
			"default":false,
			"name":"plotHStatistics",
			"type":"Boolean"
		},

		{
			"name": "methodCV",
			"type": "Boolean",
			"default": false
		},

		{
			"name":"value_numberOfFold",
			"type":"Integer",
			"min":0,
			"default":3
		},

		{
			"name": "OOBBox",
			"type": "List",
			"options": [ "auto_subsample","manual_subsample" ],
			"default": "auto_subsample"
		},

		{
			"value":50,
			"format": "",
			"name":"value_subsample",
			"type":"Number",
			"min":0,
			"max":100
		},

		{
			"name": "testBox",
			"type": "List",
			"options": [ "auto_percentageTrain", "manual_percentageTrain"],
			"default": "auto_percentageTrain"
		},

		{
			"format": "",
			"name":"value_percentageTraining",
			"type":"Number",
			"min":0,
			"max":100,
			"value":50
		},


		{
			"name": "TreeStructureBox",
			"type": "List",
			"options": [ "auto_TS","manual_TS", "optimized_TS"],
			"default": "auto_TS"
		},

		{
			"name":"value_depthOfTree",
			"type":"Integer",
			"min":1,
			"default":1
		},

		{
			"name":"value_depthOfTreeMin",
			"type":"Integer",
			"min":1,
			"default":1
		},

		{
			"name":"value_depthOfTreeMax",
			"type":"Integer",
			"min":1,
			"default":5
		},

		{
			"name":"value_depthOfTreeStep",
			"type":"Integer",
			"min":1,
			"default":1
		},

		{
			"name":"value_MinTermNodeSideMin",
			"type":"Integer",
			"min":1,
			"default":5
		},

		{
			"name":"value_MinTermNodeSideMax",
			"type":"Integer",
			"min":1,
			"default":20
		},

		{
			"name":"value_MinTermNodeSideStep",
			"type":"Integer",
			"min":1,
			"default":5
		},

		{
			"name":"value_MinTermNodeSide",
			"type":"Integer",
			"min":1,
			"default":5
		},

		{
			"name": "seedBox",
			"type": "List",
			"options": [ "auto_seed", "manual_seed" ],
			"default": "auto_seed"
		},

		{
			"name": "coreBox",
			"type": "List",
			"options": [ "auto_numberOfCore", "manual_numberOfCore" ],
			"default": "auto_numberOfCore"
		},

		{
			"name":"value_seed",
			"type":"Integer",
			"min":0,
			"default":1
		},

		{
			"name":"value_numberOfCore",
			"type":"Integer",
			"min":1,
			"default":1
		}

	]
}
