{
	"name": "RegressionLogistic",
	"autorun": true,
	"version": "1.00",
	"options": [
		{
			"name": "dependent",
			"type": "Variable"
		},
		{
			"name": "method",
			"type": "List",
			"default": "enter",
			"options": [ "enter", "backward", "forward", "stepwise" ]
		},
		{
			"name": "covariates",
			"type": "Variables"
		},
		{
			"name": "factors",
			"type": "Variables"
		},
		{
			"name": "wlsWeights",
			"type": "Variable"
		},
		{
			"name": "modelTerms",
			"template": [
				{
					"name": "components",
					"type": "Term"
				},
				{
					"default": false,
					"name": "isNuisance",
					"type": "Boolean"
				}
			],
			"type": "Table"
		},
		{
			"name": "includeIntercept",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "coeffEstimates",
			"type": "Boolean",
			"default": true
		},
        {
            "name": "coeffEstimatesBootstrapping",
            "type": "Boolean",
            "default": false
        },
        {
            "name": "coeffEstimatesBootstrappingReplicates",
            "type": "Integer",
            "min": 100,
            "default": 5000
        },
        {
			"name": "coeffCI",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "coeffCIInterval",
			"type": "Integer",
			"min": 1,
			"max": 99,
			"default": 95
		},
		{
			"name": "coeffCIOR",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "stdCoeff",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "oddsRatios",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "VovkSellkeMPR",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "robustSEOpt",
			"type": "Boolean",
			"default": false
		},
        {
            "name": "casewiseDiagnostics",
            "type": "Boolean",
            "default": false
        },
        {
            "name": "casewiseDiagnosticsType",
            "type": "List",
            "default": "residualZ",
            "options": [ "residualZ", "cooksDistance", "allCases" ]
        },
        {
            "name": "casewiseDiagnosticsResidualZ",
            "type": "Number",
            "value": 3,
            "min": 0
        },
        {
            "name": "casewiseDiagnosticsCooksDistance",
            "type": "Number",
            "value": 1,
            "min": 0
        },
		{
			"name": "confusionMatrixOpt",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "confusionMatrixProportions",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "AUC",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "Sens",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "Spec",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "Prec",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "Fmsr",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "BrierScr",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "Hmsr",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "estimatesPlotsOpt",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "estimatesPlotsCI",
			"type": "Integer",
			"min": 1,
			"max": 99,
			"default": 95
		},
		{
			"name": "showPoints",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "predictedPlotOpt",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "predictorPlotsOpt",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "squaredPearsonPlotOpt",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "residualType",
			"options": [ "deviance", "pearson" ],
			"default": "deviance",
			"type": "List"
		},
		{
			"name": "factorDescriptivesOpt",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "plotWidth",
			"type": "Integer",
			"default": 480
		},
		{
			"name": "plotHeight",
			"type": "Integer",
			"default": 320
		}
	]
}
